views:
  - type: table
    name: Table
    filters:
      and:
        - student == "Myllena"
    order:
      - date
      - conteúdo
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
      - property: conteúdo
        direction: ASC
    limit: 10
    columnSize:
      note.data: 173
      note.conteúdo: 303
