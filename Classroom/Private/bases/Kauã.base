views:
  - type: table
    name: Table
    filters:
      or:
        - or:
            - title == "Kauã"
            - student == "Kauã"
    order:
      - date
      - conteúdo
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
      - property: conteúdo
        direction: ASC
    limit: 10
    columnSize:
      note.data: 182
      note.conteúdo: 379
      file.name: 119
