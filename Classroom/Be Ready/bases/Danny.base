views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Danny"]
    order:
      - date
      - nivel
      - conteúdo
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
    columnSize:
      note.nivel: 168
      note.conteúdo: 248
