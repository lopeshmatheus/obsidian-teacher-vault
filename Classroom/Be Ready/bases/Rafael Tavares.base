views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Rafael Tavares"]
    order:
      - date
      - conteúdo
      - material
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: ASC
      - property: nivel
        direction: ASC
    columnSize:
      note.conteúdo: 232
      note.material: 116
      file.name: 126
