views:
  - type: table
    name: Table
    filters:
      and:
        - title == "London Classroom"
    order:
      - date
      - conteúdo
      - Alunos
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: conteúdo
        direction: DESC
      - property: data
        direction: DESC
      - property: Alunos
        direction: DESC
    columnSize:
      note.date: 130
      note.conteúdo: 119
      note.Alunos: 250
      file.name: 122
