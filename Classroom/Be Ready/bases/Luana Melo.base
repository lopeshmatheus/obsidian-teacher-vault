views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Luana Melo"]
    order:
      - data
      - nivel
      - conteúdo
      - file.name
    sort:
      - property: data
        direction: DESC
      - property: conteúdo
        direction: DESC
      - property: file.name
        direction: ASC
    columnSize:
      note.nivel: 158
      note.conteúdo: 253
      file.name: 132
