views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Luana Melo"]
    order:
      - date
      - nivel
      - conteúdo
      - file.name
    sort:
      - property: file.name
        direction: DESC
      - property: date
        direction: DESC
      - property: data
        direction: DESC
      - property: conteúdo
        direction: DESC
    columnSize:
      note.nivel: 158
      note.conteúdo: 136
      file.name: 132
