views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Iago"]
    order:
      - conteúdo
      - nivel
      - file.name
      - date
    sort:
      - property: data
        direction: DESC
      - property: file.name
        direction: DESC
    columnSize:
      note.conteúdo: 122
      note.nivel: 200
