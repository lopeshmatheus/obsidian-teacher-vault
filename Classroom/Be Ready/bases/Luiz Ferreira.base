views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Luiz Ferreira"]
    order:
      - date
      - conteúdo
      - nivel
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
      - property: nivel
        direction: DESC
    limit: 10
    columnSize:
      note.date: 157
      note.conteúdo: 132
      note.nivel: 260
      file.name: 140
