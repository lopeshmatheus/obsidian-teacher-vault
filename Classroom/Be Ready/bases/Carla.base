views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Carla"]
    order:
      - date
      - conteúdo
      - nivel
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
      - property: file.name
        direction: ASC
    columnSize:
      note.data: 149
      note.conteúdo: 164
      note.nivel: 262
