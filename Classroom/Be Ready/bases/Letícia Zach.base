views:
  - type: table
    name: Table
    filters:
      and:
        - Alunos == ["Letícia"]
    order:
      - date
      - nivel
      - conteúdo
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: ASC
      - property: conteúdo
        direction: ASC
    columnSize:
      note.nivel: 177
      note.conteúdo: 115
      file.name: 117
