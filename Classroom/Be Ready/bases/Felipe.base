views:
  - type: table
    name: Table
    filters:
      and:
        - file.hasTag("be-ready-classes")
        - Alunos == ["Felipe"]
    order:
      - date
      - conteúdo
      - nivel
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
    limit: 10
    columnSize:
      note.date: 117
      note.conteúdo: 113
      note.nivel: 106
