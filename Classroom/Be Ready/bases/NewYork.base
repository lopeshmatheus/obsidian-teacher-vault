views:
  - type: table
    name: Table
    filters:
      and:
        - title == "New York Classroom"
    order:
      - date
      - conteúdo
      - Alunos
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: conteúdo
        direction: ASC
      - property: data
        direction: DESC
    limit: 10
    columnSize:
      note.conteúdo: 145
      note.Alunos: 308
      file.name: 123
