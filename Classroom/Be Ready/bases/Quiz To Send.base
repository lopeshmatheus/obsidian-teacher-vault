views:
  - type: table
    name: Table
    filters:
      and:
        - file.hasTag("be-ready-classes")
        - quiz != true
        - file.hasProperty("quiz")
    order:
      - quiz
      - date
      - title
      - conteúdo
      - gemini-quiz
      - file.name
    sort:
      - property: date
        direction: DESC
      - property: data
        direction: DESC
    columnSize:
      note.quiz: 61
      note.date: 115
      note.title: 168
      note.conteúdo: 94
      note.gemini-quiz: 121
      file.name: 77
